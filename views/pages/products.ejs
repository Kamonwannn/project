<% include ../partials/header %>
<H2> Search Here</H2>
<input class="form-control" id="myInput" type="text" placeholder="Search..">
 <br>
            <table class="table">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Title</th>
                        <th>Price</th>
                        
                    </tr> 
                </thead>
                <tbody>
                    <% products.forEach(function(product) { %>
                    <tr>
                        <td scope="row"><%= product.id %></td>
                        <td><%= product.title %></td>
                        <td><%= product.price %></td>
                        <td><a class="btn btn-primary" href="/products/<%= product.id %>" role="button"> Edit </a></td>
                        <td><a class="btn btn-danger" href="/products/delete<%= product.id %>" role="button"> Delete </a></td>
                        
                    </tr>
                     <% }); 
                </tbody>
            </table>


 <% include ../partials/footer %>