<% include ../partials/header %>
  
            <table class="table">
                <thead>
                    <tr>
                        <th>user ID</th>
                        <th>Emil</th>
                        <th>Password</th>
                    </tr>
                </thead>
                <tbody>
                 <% users.forEach(function(user) { %>
                    <tr>
                        <td scope="row"><%= user.id %></td>
                        <td><%= user.email %></td>
                        <td><%= user.password %></td>
                        <td><a class="btn btn-primary" href="/users/<%= user.id %>" role="button"> Edit </a></td>
                        <td><a class="btn btn-danger" href="/users/delete<%= user.id %>" role="button"> Delete </a></td>
                    </tr>
                     <% }); %>
                </tbody>
            </table>


 <% include ../partials/footer %>